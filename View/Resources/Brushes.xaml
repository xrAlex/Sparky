<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:o="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="LightGray" Color="#ededed" o:Freeze ="true"/>
    <SolidColorBrush x:Key="TLightGray" Color="#ededed" Opacity="0" o:Freeze ="true"/>

    <SolidColorBrush x:Key="MiddleGray" Color="#FFD6D6D6" o:Freeze ="true"/>
    <SolidColorBrush x:Key="TMiddleGray" Color="#FFD6D6D6" Opacity="0" o:Freeze ="true"/>

    <SolidColorBrush x:Key="LowGray" Color="#FFAAAAAA" o:Freeze ="true"/>
    <SolidColorBrush x:Key="TLowGray" Color="#FFAAAAAA" Opacity="0" o:Freeze ="true"/>

    <SolidColorBrush x:Key="Gray" Color="#FFBBBBBB" o:Freeze ="true"/>
    <SolidColorBrush x:Key="TGray" Color="#FFBBBBBB" Opacity="0" o:Freeze ="true"/>

    <SolidColorBrush x:Key="DeepGray" Color="#FF5B5B5B" o:Freeze ="true"/>
    <SolidColorBrush x:Key="TDeepGray" Color="#FF5B5B5B" Opacity="0" o:Freeze ="true"/>

    <SolidColorBrush x:Key="Black" Color="#FF292929" o:Freeze ="true"/>
    <SolidColorBrush x:Key="TBlack" Color="#FF292929" Opacity="0" o:Freeze ="true"/>

    <SolidColorBrush x:Key="LightBlack" Color="#FF1F1F1F" o:Freeze ="true"/>
    <SolidColorBrush x:Key="TLightBlack" Color="#FF1F1F1F" Opacity="0" o:Freeze ="true"/>

</ResourceDictionary>